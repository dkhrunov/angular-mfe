{
	"npmScope": "nx-mfe",
	"affected": {
		"defaultBase": "development"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"projects": {
		"client-auth": {
			"tags": ["client"]
		},
		"client-auth-mfe": {
			"tags": ["client", "remote-mfe"]
		},
		"client-auth-mfe-e2e": {
			"tags": ["client"],
			"implicitDependencies": ["client-auth-mfe"]
		},
		"client-config": {
			"tags": ["client"]
		},
		"client-core": {
			"tags": ["client"]
		},
		"client-dashboard-mfe": {
			"tags": ["client", "remote-mfe"]
		},
		"client-dashboard-mfe-e2e": {
			"tags": ["client"],
			"implicitDependencies": ["client-dashboard-mfe"]
		},
		"client-mfe": {
			"tags": ["client"]
		},
		"client-shell-app": {
			"tags": ["client", "host-mfe"]
		},
		"client-shell-app-e2e": {
			"tags": ["client"],
			"implicitDependencies": ["client-shell-app"]
		},
		"client-token-manager": {
			"tags": ["client"]
		},
		"server": {
			"tags": ["server"]
		},
		"server-domains": {
			"tags": ["server"]
		},
		"server-mailer": {
			"tags": ["server"]
		},
		"shared-data-access": {
			"tags": ["server"]
		}
	}
}
