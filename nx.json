{
  "npmScope": "nx-mfe",
  "affected": {
    "defaultBase": "development"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "client-auth": {
      "tags": [
        "scope:client"
      ]
    },
    "client-auth-mfe": {
      "tags": [
        "scope:client",
        "mfe:remote"
      ]
    },
    "client-auth-mfe-e2e": {
      "tags": [
        "scope:client"
      ],
      "implicitDependencies": [
        "client-auth-mfe"
      ]
    },
    "client-common": {
