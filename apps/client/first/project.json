{
	"projectType": "application",
	"sourceRoot": "apps/client/first/src",
	"prefix": "nx-mfe",
	"targets": {
		"build": {
			"executor": "@nrwl/angular:webpack-browser",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/apps/client/first",
				"index": "apps/client/first/src/index.html",
				"main": "apps/client/first/src/main.ts",
				"polyfills": "apps/client/first/src/polyfills.ts",
				"tsConfig": "apps/client/first/tsconfig.app.json",
				"inlineStyleLanguage": "scss",
				"assets": ["apps/client/first/src/favicon.ico", "apps/client/first/src/assets"],
				"styles": ["apps/client/first/src/styles.scss"],
				"scripts": [],
				"customWebpackConfig": {
					"path": "apps/client/first/webpack.config.js"
				}
			},
			"configurations": {
				"production": {
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "500kb",
							"maximumError": "1mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "2kb",
							"maximumError": "4kb"
						}
					],
					"fileReplacements": [
						{
							"replace": "apps/client/first/src/environments/environment.ts",
							"with": "apps/client/first/src/environments/environment.prod.ts"
						}
					],
					"outputHashing": "all",
					"customWebpackConfig": {
						"path": "apps/client/first/webpack.prod.config.js"
					}
				},
				"development": {
					"buildOptimizer": false,
					"optimization": false,
					"vendorChunk": true,
					"extractLicenses": false,
					"sourceMap": true,
					"namedChunks": true
				}
			},
			"defaultConfiguration": "production"
		},
		"serve": {
			"executor": "@nrwl/angular:webpack-server",
			"configurations": {
				"production": {
					"browserTarget": "client-first:build:production"
				},
				"development": {
					"browserTarget": "client-first:build:development"
				}
			},
			"defaultConfiguration": "development",
			"options": {
				"port": 5201,
				"publicHost": "http://localhost:5201"
			}
		},
		"extract-i18n": {
			"executor": "@angular-devkit/build-angular:extract-i18n",
			"options": {
				"browserTarget": "client-first:build"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": ["apps/client/first/**/*.ts", "apps/client/first/**/*.html"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/apps/client/first"],
			"options": {
				"jestConfig": "apps/client/first/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"serve-static": {
			"executor": "@nrwl/angular:file-server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "client-first:build",
				"port": 5201
			},
			"configurations": {
				"development": {
					"buildTarget": "client-first:build:development"
				},
				"production": {
					"buildTarget": "client-first:build:production"
				}
			}
		}
	},
	"tags": ["scope:client", "mfe:remote"]
}
