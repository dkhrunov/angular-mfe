{
	"sourceRoot": "apps/client/host-app-e2e/src",
	"projectType": "application",
	"targets": {
		"e2e": {
			"executor": "@nrwl/cypress:cypress",
			"options": {
				"cypressConfig": "apps/client/host-app-e2e/cypress.json",
				"devServerTarget": "client-host-app:serve:development"
			},
			"configurations": {
				"production": {
					"devServerTarget": "client-host-app:serve:production"
				}
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/client/host-app-e2e/**/*.{js,ts}"]
			}
		}
	},
	"tags": [],
	"implicitDependencies": ["client-host-app"]
}
